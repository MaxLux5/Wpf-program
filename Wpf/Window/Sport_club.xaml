<Window x:Class="Wpf.Sport_club"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Sport_club" Height="450" Width="800"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <DataGrid x:Name="Sport_clubGrid" d:ItemsSource="{d:SampleData ItemCount=5}" Loaded="DataGrid_Loaded" Height="200" Margin="0,0,0,234" AutoGenerateColumns="True"/>
        <Button Content="Назад" HorizontalAlignment="Left" Height="30" Margin="20,365,0,0" VerticalAlignment="Top" Width="90" Click="Button_Back"/>

    </Grid>
</Window>
